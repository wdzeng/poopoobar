{
  "name": "poopoobar",
  "version": "0.2.12",
  "description": "A cool CLI progress bar",
  "author": {
    "name": "hyperbola",
    "email": "me@hyperbola.me",
    "url": "https://blog.hyperbola.me"
  },
  "license": "MIT",
  "homepage": "https://github.com/wdzeng/poopoobar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdzeng/poopoobar.git"
  },
  "bugs": "https://github.com/wdzeng/poopoobar/issues",
  "keywords": [
    "cli",
    "tty",
    "progress",
    "progressbar",
    "bar",
    "status",
    "statusbar",
    "utility",
    "widget"
  ],
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "./src/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "lint": "eslint src/ --ext .ts,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@types/node": "^20.9.3",
    "eslint": "^8.54.0",
    "eslint-config-wdzeng": "github:wdzeng/eslint-config#semver:^0.6.0",
    "prettier": "^3.1.0",
    "tsc-alias": "^1.8.8",
    "tsup": "^8.0.1",
    "tsx": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
