{
  "name": "poopoobar",
  "version": "0.1.3",
  "description": "A cool CLI progress bar",
  "author": {
    "name": "hyperbola",
    "email": "me@hyperbola.me",
    "url": "https://blog.hyperbola.me"
  },
  "license": "MIT",
  "homepage": "https://github.com/wdzeng/poopoobar",
  "bugs": {
    "url": "https://github.com/wdzeng/poopoobar/issues",
    "email": "me@hyperbola.me"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "src",
    "dist",
    "docs",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "lint": "eslint src/ --ext .ts,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "eslint": "^8.53.0",
    "eslint-config-wdzeng": "github:wdzeng/eslint-config#semver:^0.5.1",
    "prettier": "^3.0.3",
    "tsc-alias": "^1.8.8",
    "tsup": "^7.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  }
}
